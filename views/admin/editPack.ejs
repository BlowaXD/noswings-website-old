<!DOCTYPE html>
<html>
    <head>
        <link href="/assets/plugins/jquery-datatable/media/css/dataTables.bootstrap.min.css" rel="stylesheet" type="text/css" />
        <link href="/assets/plugins/jquery-datatable/extensions/FixedColumns/css/dataTables.fixedColumns.min.css" rel="stylesheet" type="text/css" />
        <link href="/assets/plugins/datatables-responsive/css/datatables.responsive.css" rel="stylesheet" type="text/css" media="screen" />
        <% include shared/head.ejs %>
    </head>

    <body class="fixed-header menu-pin">
    <% include shared/sidepanel.ejs %>
    <!-- START PAGE-CONTAINER -->
    <div class="page-container">
        <% include shared/header.ejs %>
        <!-- START PAGE CONTENT WRAPPER -->
        <div class="page-content-wrapper">
            <!-- START PAGE CONTENT -->
            <div class="content">
                <!-- START CONTAINER FLUID -->
                <div class=" container-fluid   container-fixed-lg">
                    <o class="breadcrumb"></o>
                    <!-- BEGIN PlACE PAGE CONTENT HERE -->
                    <div class="row">
                        <div class="col-lg-12">
                            <!-- START card -->
                            <div class="card card-transparent">
                                <div class="card-header">
                                    <div class="card-title">Liste des patchs
                                    </div>
                                </div>
                                <div class="card-block">
                                    <div class="table-responsive">
                                        <table class="table table-hover table-condensed table-detailed" id="detailedTable">
                                            <thead>
                                            <tr>
                                                <th style="width:10%">ID</th>
                                                <th style="width:40%">HASH CLIENT</th>
                                                <th style="width:25%">IP</th>
                                                <th style="width:25%">PORT</th>
                                                <th style="width:25%">MULTICLIENT</th>
                                                <th style="width:25%">SUPPRIMER</th>
                                            </tr>
                                            </thead>
                                            <tbody>
                                            <% for (var i in locals.patch) { %>
                                            <tr>
                                                <td class="v-align-middle semi-bold"><%- locals.patch[i].Id %></td>
                                                <td class="v-align-middle"><%- locals.patch[i].Hash %></td>
                                                <td class="v-align-middle semi-bold"><%- locals.patch[i].Ip %></td>
                                                <td class="v-align-middle"><%- locals.patch[i].Port %></td>
                                                <td class="v-align-middle"><%- locals.patch[i].Multiclient === true ? '<i class="fa fa-check"></i>' : '<i class="fa fa-times"></i>' %></td>
                                                <td class="v-align-middle"><btn class="btn btn-xs btn-primary"><i class="fa fa-trash-o"></i></btn></td>
                                            </tr>
                                            <% } %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <!-- END card -->
                        </div>
                    </div>
                    <!-- END PLACE PAGE CONTENT HERE -->
                </div>
                <!-- END CONTAINER FLUID -->
            </div>
            <!-- END PAGE CONTENT -->
            <% include shared/copyright.ejs %>
        </div>
        <!-- END PAGE CONTENT WRAPPER -->
    </div>
    <!-- END PAGE CONTAINER -->

    <script src="/assets/plugins/jquery-datatable/media/js/jquery.dataTables.min.js" type="text/javascript"></script>
    <script src="/assets/plugins/jquery-datatable/extensions/TableTools/js/dataTables.tableTools.min.js" type="text/javascript"></script>
    <script src="/assets/plugins/jquery-datatable/media/js/dataTables.bootstrap.js" type="text/javascript"></script>
    <script src="/assets/plugins/jquery-datatable/extensions/Bootstrap/jquery-datatable-bootstrap.js" type="text/javascript"></script>
    <script src="/assets/plugins/datatables-responsive/js/datatables.responsive.js" type="text/javascript"></script>
    <script src="/assets/plugins/datatables-responsive/js/lodash.min.js" type="text/javascript"></script>
    <script src="/assets/js/tables.js" type="text/javascript"></script>
    <% include shared/javascript.ejs %>
    </body>
</html>
